<?xml version="1.0" encoding="utf-8"?>
<romset>
   <resources>
      <rom id="romimage"  file="romc.bin"/>
      <rom id="gromimage" file="romg.bin"/>
   </resources>
   <configuration>
      <!-- We're using 8K GROM chips. -->
      <pcb type="gromemu">
         <socket id="rom_socket"  uses="romimage"/>
         <socket id="grom_socket" uses="gromimage"/>
      </pcb>
   </configuration>
</romset>
